class Main {

    /** Initializes a new game and starts running it. */
    function void main() {
        var Game game;
        var boolean restart;
        
        let restart = true;
        
        while (restart) {
            let restart = false;
            let game = Game.new();
            do game.run();
            let restart = game.shouldRestart();
            do game.dispose();
        }
        return;
    }
}